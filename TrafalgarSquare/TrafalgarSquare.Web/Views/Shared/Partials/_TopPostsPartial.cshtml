@model IEnumerable<TrafalgarSquare.Web.ViewModels.TopPostViewModel>
<aside class="col-md-2">
    <h2>Top 10 Jokes</h2>
    
    @if (!Model.Any())
    {
        @:<p>No Posts for Rank!</p>
    }
    else
    {
        <div class="list-group">
            @Html.DisplayFor(t => t)
        </div>
    }
</aside>